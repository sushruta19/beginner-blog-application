<%- include("partials/header") -%>



<h1>Home</h1>
<p> <%= staticContent %></p>

<% posts.forEach(post => { %>
    <h2><%= post.title %></h2>
    <p><%= _.truncate(post.content, {'length':100}) %>
      <a href= "/posts/<%= post.title %>" >Read More</a>
    </p>
<% }) %>

<%- include("partials/footer") -%>